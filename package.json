{
  "name": "@mapsindoors/midt",
  "version": "10.0.5",
  "description": "Design Tokens for MapsIndoors",
  "author": "MapsPeople A/S",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/mapsindoors/midt.git"
  },
  "keywords": [
    "mapsindoors",
    "design",
    "tokens",
    "system"
  ],
  "sass": "mixins/_index.scss",
  "style": "midt.css",
  "scripts": {
    "build": "npm run clean && node_modules/.bin/style-dictionary build && sass midt.scss:midt.css midt-helpers.scss:midt-helpers.css --no-source-map",
    "clean": "rm -rf build",
    "postversion": "git push -u origin --tags && npm publish --access public",
    "sass-watch": "sass --watch midt.scss:midt.css midt-helpers.scss:midt-helpers.css --no-source-map",
    "sass": "sass midt.scss:midt.css midt-helpers.scss:midt-helpers.css --no-source-map",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm run build && chokidar \"properties/**/*.json\" -c \"npm run build\""
  },
  "dependencies": {},
  "devDependencies": {
    "chokidar-cli": "^2.1.0",
    "sass": "^1.23.7",
    "style-dictionary": "^2.8.3"
  }
}
